
prop Countable {
    int n
} with {
    func display_count() void {
        print "Count is " + str(n);
    }
}

struct Farmer with Countable as { n=sheep } {
    int sheep
}

val farmer = Farmer { sheep=3i };

print farmer.n; // no
farmer.display_count(); // yes

func test(Countable c) void {
    print c.n; // yes
    c.display_count(); // yes
}

test(farmer);
