
prop Countable {
    int n
}

prop Sizeable {
    int size
}

struct A with Countable as { n=sheep }, Sizeable as { size=sheep } {
    num x,
    int sheep
}

func count(Countable c) void {
    print "Count is " + str(c.n);
}

func size(Sizeable s) void {
    print "Size is " + str(s.size);
}

val a = A { x=3.14, sheep=2i };
count(a);
size(a);
