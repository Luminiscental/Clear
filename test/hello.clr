
func a() void {
    print "a()";
}

func b(int x) void {
    print "b(" + str(x) + ")";
}

val c := 2, "two", -5i * 3i;

val test : (bool | num)? = 3;
val user := case test as condition {
    bool: a,
    num: c,
    else: b
};

case user {
    func() void: user(),
    func(int) void: user(3i),
    num, str, int: b(4i)
};
