
var logger = func() "[No log]";
print logger();

{
    val newline = "
";

    func greeting(str name) str {
        return "Hello " + name + "!";
    }

    var msg = greeting("World");

    func log_msg() str {
        return "[Logging the message]" + newline + msg + newline + "[Done]";
    }

    print log_msg();

    msg = "...";

    logger = log_msg;
}

print logger();
