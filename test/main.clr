
func printFac(int n) {
    func fac(int n) {
        var counter = n;
        var result = 1;
        while counter > 1i {
            result = result * num(counter);
            counter = counter - 1i;
        }
        return result;
    }
    return str(n) + "! = " + str(fac(n));
}

val MAX = 10i;
var i = 0i;
while i < MAX {
    print printFac(i);
    i = i + 1i;
}

