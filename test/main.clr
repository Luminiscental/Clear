
func oldFunc() int {
    return 0i;
}

var three = oldFunc;
var four = oldFunc;

{
    func makeFunc(int n) func() int {
        func result() int {
            return n;
        }
        return result;
    }

    three = makeFunc(3i);
    four = makeFunc(4i);
}

print "three() = " + str(three());
print "four() = " + str(four());
